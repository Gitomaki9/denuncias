{
  "github": {
    "silent": true
  },
  "public": true,
  "trailingSlash": false,
  "redirects": [],
  "rewrites": [
    {
      "source": "/css/:match*",
      "destination": "/css/:match*"
    },
    {
      "source": "/js/:match*", 
      "destination": "/js/:match*"
    },
    {
      "source": "/assets/:match*",
      "destination": "/assets/:match*"
    },
    {
      "source": "/:path*",
      "destination": "/html/:path*.html"
    }
  ]
}
