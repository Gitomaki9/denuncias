{
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/",
      "dest": "/html/index.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/html/$1.html",
      "status": 200
    }
  ]
}
