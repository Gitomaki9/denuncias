{
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/assets/([^/]+\.(svg|png|jpg|jpeg|gif))",
      "destination": "/assets/$1"
    },
    {
      "source": "/assets/images/(.*)",
      "destination": "/assets/images/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/js/$1"
    },
    {
      "source": "/",
      "destination": "/html/index.html"
    },
    {
      "source": "/login",
      "destination": "/html/login.html"
    },
    {
      "source": "/registro",
      "destination": "/html/registro.html"
    },
    {
      "source": "/contactanos",
      "destination": "/html/contactanos.html"
    },
    {
      "source": "/normativa",
      "destination": "/html/normativa.html"
    },
    {
      "source": "/perfil",
      "destination": "/html/perfil.html"
    },
    {
      "source": "/denuncias",
      "destination": "/html/denuncias.html"
    },
    {
      "source": "/crear_denuncia",
      "destination": "/html/crear_denuncia.html"
    },
    {
      "source": "/(.*)",
      "destination": "/html/$1.html"
    }
  ]
}
